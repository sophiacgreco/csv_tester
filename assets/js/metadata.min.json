---
# metadata for table ajax
---
{% if site.data.theme.data-child-objects == false %}
{%- assign items = site.data[site.metadata] | where_exp: 'item','item.parentid == nil' -%}
{% else %}
{%- assign items = site.data[site.metadata] | where_exp: 'item','item.objectid' -%}
{% endif %}
{%- assign fields = site.data.config-table | map: 'field' -%}
{ "objects": [
{%- for item in items -%}[ {% for f in fields %}{% if item[f] %}{{ item[f] | jsonify }}{% else %}""{% endif %},{% endfor %} "/items/{% if item.parentid %}{{ item.parentid }}.html#{{ item.objectid }}{% else %}{{ item.objectid }}.html{% endif %}" ]{% unless forloop.last %},{% endunless %}
{% endfor %}
] }